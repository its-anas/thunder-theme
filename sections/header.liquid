                            <svg
                              width="8"
                              height="5"
                              viewBox="0 0 8 5"
                              fill="none"
                              xmlns="http://www.w3.org/2000/svg"
                            >
                              <path d="M1 1L4.25787 3.25787L7.51575 1" stroke="white" stroke-width="1.5"/>
                            </svg>
                          </div>
                        {% endif %}
                      </div>
                      <div class="menu-mobile__parent-childs {% if menu_parent.links.size > 0 %}border{% endif %}">
                        {% for menu_child in menu_parent.links %}
                          {% assign menu_child_title = menu_child.title
                            | downcase
                            | remove: ' '
                            | remove: '\'
                            | remove: '!'
                            | remove: '@'
                            | remove: 'Â£'
                            | remove: '$'
                            | remove: '%'
                            | remove: '^'
                            | remove: '&'
                            | remove: '*'
                            | remove: '('
                            | remove: ')'
                            | remove: '-'
                            | remove: '+'
                            | remove: '='
                            | remove: '{'
                            | remove: '}'
                            | remove: ':'
                            | remove: '|'
                            | remove: '\'
                            | remove: '<'
                            | remove: '>'
                            | remove: ','
                            | remove: '.'
                            | remove: '?'
                            | remove: '~'
                            | remove: '`'
                            | remove: '\'
                            | remove: '/'
