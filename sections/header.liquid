                                @media screen and (max-width: 1440px) {
                                  .menu__dropdown.menu_parent_title--{{ menu_parent_title }} {
                                  width: 600px;
                                  transform: translateX(-50%) translateY(-10%);
                                  left: 50%;
                                  }

                                  .menu__parent:hover .menu__dropdown.menu_parent_title--{{ menu_parent_title }} {
                                    transform: translateX(-50%) translateY(0%);
                                  }
                                }
                              {% else %}
                                 .menu__dropdown.menu_parent_title--{{ menu_parent_title }} {
                                  width: 600px;
                                  transform: translateX(-50%) translateY(-10%);
                                  left: 50%;
                                  }
                                  .menu__parent:hover .menu__dropdown.menu_parent_title--{{ menu_parent_title }} {
                                    transform: translateX(-50%) translateY(0%);
                                  }
