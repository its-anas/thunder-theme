{% style %}
  .header__content {
    background-color: {{ section.settings.header_background_color }};
   }

  .menu__parent-title {
    color: {{ section.settings.content_color }};
  }

  @media screen and (min-width: 1024px) {
      {%- if section.settings.menu_layout == "logo_first_navigation_inline" -%}
      .header__menu {
              width: 100%;
      }
      .header__logo {
              max-width: 250px;
      }
      {%- endif -%}

      {%- if section.settings.menu_layout == "drawer" -%}
        .header__menu {
               flex-grow: 1;
        }
      {%- endif -%}
      .header__menu__content  {
          {%- if section.settings.menu_layout == "logo_first_navigation_inline" -%}
              padding: 0px 2rem;
              width: 100%;
              justify-content: flex-start;
          {%- else -%}
              justify-content: center;
          {%- endif -%}
      }

      {%- if section.settings.menu_layout == "drawer" -%}
          .header__logo {
              justify-content: center;
          }
      {%- endif -%}

      {% if section.settings.menu_layout == "navigation_first" or section.settings.menu_layout == "drawer" %}
          .header__logo {
              order: 2;
              justify-content: center;
          }
          .header__menu {
              order: 1;
          }
          .header__menu > * {
                justify-content: flex-start;
          }
      {%- elsif section.settings.menu_layout == "logo_first" -%}
          .header__logo {
              order: 1;
              justify-content: flex-start;
          }
          .header__menu {
              order: 2;
          }
      {%- endif -%}
  }
{% endstyle %}

<section class="header">
  <div class="header__content">
    <div class="header__logo">
      <a href="/" class="header__logo-container">
        {% if section.settings.logo_type == 'svg' %}
          {% if section.settings.logo_svg == blank %}
            <p style="text-transform: uppercase; font-size: 1.2rem; font-weight: bold; text-align: center;">
              {{ shop.name }}
            </p>
          {% else %}
            {{ section.settings.logo_svg }}
          {% endif %}
        {% elsif section.settings.logo_type == 'image' %}
          {% if section.settings.logo_image == blank %}
            <p style="text-transform: uppercase; font-size: 1.2rem; font-weight: bold; text-align: center;">
              {{ shop.name }}
            </p>
          {% else %}
            <img
              srcset="{{ section.settings.logo_image | image_url }}"
              loading="lazy"
              alt="{{ section.settings.logo_image | escape }}"
              width="{{ section.settings.logo_image.width }}"
              height="{{ section.settings.logo_image.height }}"
              class="full"
            >
          {% endif %}
        {% endif %}
      </a>
    </div>
    <div class="header__menu">
      <menu-mobile>
        <openable-element class="active">
          <div class="menu-mobile">
            <div class="menu-mobile__container">
              <div class="menu-mobile__head">
                <div class="menu-mobile__title">
                  <h6>Menu</h6>
                </div>
                <div id="close-icon">
                  {% render 'icons', icon: 'close' %}
                </div>
              </div>
              <div class="menu-mobile__content">
                <div class="menu-mobile__parents">
                  {% for menu_parent in section.settings.menu.links %}
                    <div class="menu-mobile__parent">
